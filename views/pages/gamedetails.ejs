<% include ../partials/header %>
<div class="container">
    <h1>Game : <%=detail.Name%></h1>
    <form action="/classsave" method="POST">

        <div class="form-group">
          <label for="">Name</label>
          <input type="text" class="form-control" value="<%=detail.Name%>" name="name" id="" aria-describedby="helpId" readonly
            placeholder="">
          <small id="helpId" class="form-text text-muted">Help text</small>
        </div>
        <div class="form-group">
          <label for="">Type</label>
          <input type="text" class="form-control" value="<%=detail.Type%>" name="type" id="" readonly
            aria-describedby="helpId" placeholder="">
          <small id="helpId" class="form-text text-muted">Help text</small>
        </div>
        <div class="form-group">
          <label for="">Rate</label>
          <input type="text" class="form-control" value="<%=detail.Rate%>" name="rate" id="" aria-describedby="helpId" readonly
            placeholder="">
          <small id="helpId" class="form-text text-muted">Help text</small>
        </div>
        <div class="form-group">
            <label for="">Price</label>
            <input type="text" class="form-control" value="<%=detail.Price%>" name="price" id="" aria-describedby="helpId" readonly
              placeholder="">
            <small id="helpId" class="form-text text-muted">Help text</small>
          </div>
          <a name="" id="" class="btn btn-primary" href="/game" role="button">Back</a>
      </form>


   
</div>
<% include ../partials/footer %>